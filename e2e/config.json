{
	"rootDir": "../",
	"setupFilesAfterEnv": ["./e2e/init.js"],
	"preset": "react-native",
	"forceExit": true,
	"transform": {
		"^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
	},
	"testRunner": "jest-circus/runner",
	"testTimeout": 120000,
	"testRegex": "\\.spec\\.js$",
	"reporters": ["detox/runners/jest/streamlineReporter"],
	"verbose": true
}
